                                            <th scope="col">EventID</th>
                                            <th scope="col">PredictedValue</th>
                                            <th scope="col">Event Description</th>
                                            <th scope="col">Details</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% 
                                        let filteredEvents = events.filter(event => event.PredictedValue > 0);
                                        for(let i = 0; i < Math.min(10, filteredEvents.length); i++) { 
                                            let event = filteredEvents[i];
                                            let index = i; 
                                        %>
                                            <tr>
                                                <td><%= event.EventID %></td>
                                                <td>
                                                    <%= event.PredictedValue %>
                                                    <% if (event.PredictedValue == 0) { %>
                                                        <span class="badge bg-success">Safe</span>
                                                    <% } else if (event.PredictedValue <= 2) { %>
                                                        <span class="badge bg-warning">Warning</span>
                                                    <% } else { %>
                                                        <span class="badge bg-danger">Danger</span>
                                                    <% } %>
                                                </td>
                                                <td style="max-width: 1000px; word-wrap: break-word;"><%= event.Description ? event.Description : 'Unknown' %></td>
                                                <td>
                                                    <button class="btn btn-sm btn-primary" data-bs-toggle="modal" data-bs-target="#details-<%= index + 1 %>">Details</button>
                                                    <div class="modal fade" id="details-<%= index + 1 %>" tabindex="-1" aria-labelledby="details-<%= index + 1 %>-label" aria-hidden="true">
                                                        <div class="modal-dialog modal-lg">
                                                            <div class="modal-content bg-secondary text-white">
                                                                <div class="modal-header">
                                                                    <h5 class="modal-title" id="details-<%= index + 1 %>-label" style="padding: 10px;">Event Details</h5>
                                                                    <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="Close"></button>
                                                                </div>
                                                                <div class="modal-body">
                                                                    <div style="overflow: auto; max-height: 400px; font-size: 1.2em;">  
                                                                        <p><strong>EventID:</strong> <%= event.EventID %></p>
                                                                        <p><strong>PredictedValue:</strong> <%= event.PredictedValue %></p>
                                                                        <p><strong>Description:</strong> <%= event.Description %></p>
                                                                        <p><strong>SourceName:</strong> <%= event.SourceName %></p>
                                                                        <p><strong>Status:</strong> <%= event.Level %></p>
                                                                        <p><strong>Channel:</strong> <%= event.Channel %></p>
                                                                        <p><strong>Message:</strong>
                                                                            <pre>
                                                                                <% 
                                                                                try {
                                                                                    var messageObj = JSON.parse(event.Message);
                                                                                    %><%= JSON.stringify(messageObj, null, 2) %><%
                                                                                } catch (error) {
                                                                                    %><%= event.Message %><%
                                                                                }
                                                                                %>
                                                                            </pre>
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <button type="button" class="btn btn-light" data-bs-dismiss="modal" style="font-size: 1.2em;">Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                      
                    <div class="container-fluid">
                        <div class="bg-secondary text-center rounded p-4">
