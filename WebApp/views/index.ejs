                                            <th scope="col">PID</th>
                                            <th scope="col">Process</th>
                                            <th scope="col">Protocol</th>
                                            <th scope="col">StartTime</th>
                                            <th scope="col">CommunicationProtocol</th>
                                            <th scope="col">LocalIP</th>
                                            <th scope="col">LocalPort</th>
                                            <th scope="col">RemoteIP</th>
                                            <th scope="col">RemotePort</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(let i = 0; i < Math.min(10, IpLogs.length); i++) { 
                                            let log = IpLogs[i]; %>
                                            <tr>
                                                <td><%= log.PID %></td>
                                                <td><%= log.Process %></td>
                                                <td><%= log.Protocol %></td>
                                                <td><%= log.StartTime %></td>
                                                <td><%= log.CommunicationProtocol %></td>
                                                <td><%= log.LocalIP %>
                                                    <% if (isGlobalIP(log.LocalIP)) { %>
                                                        <span class="blink_me">•</span>
                                                    <% } %>
                                                </td>
                                                <td><%= log.LocalPort %></td>
                                                <td><%= log.RemoteIP %>
                                                    <% if (isGlobalIP(log.RemoteIP)) { %>
                                                        <span class="blink_me">•</span>
                                                    <% } %>
                                                </td>
                                                <td><%= log.RemotePort %></td>
                                            </tr>
                                        <% } %>
